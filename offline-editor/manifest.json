{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "author": "MediaTek Labs",
   "default_locale": "en",
   "description": "BlocklyDuino Editor for Linkit 7697.",
   "externally_connectable": {
      "matches": [ "*://MediaTek-Labs.github.io/*", "*://*.mediatek.com/*" ]
   },
   "file_handlers": {
      "text": {
         "types": [ "text/*" ]
      }
   },
   "icons": {
      "128": "media/icon_128.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "33",
   "name": "Linkit 7697 BlocklyDuino Editor",
   "offline_enabled": true,
   "permissions": [ {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "storage", "https://raw.githubusercontent.com/MediaTek-Labs/*", "alarms" ],
   "update_url": "https://github.com/MediaTek-Labs/BlocklyDuino-for-LinkIt/releases",
   "version": "2.7.5"
}
